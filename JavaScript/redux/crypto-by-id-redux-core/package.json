  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reyarqueza/react-webpack-build/issues"
  },
  "homepage": "https://github.com/reyarqueza/react-webpack-build#readme",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "prettier": "2.7.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
